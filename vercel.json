{
  "version": 2,
  "builds": [
    {
      "src": "chytozavrik/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    },
    {
      "src": "media/(.*)",
      "use": "@vercel/static",
      "config": {
        "headers": {
          "/media/(.*)": {
            "Cache-Control": "public, max-age=604800, immutable"
          }
        }
      }
    },
    {
      "src": "public/(.*)"
    },
    {
      "src": "/(.*)",
      "dest": "chytozavrik/wsgi.py"
    }
  ]
}
