{
  "version": 2,
  "builds": [
    {
      "src": "chytozavrik/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    },
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/chytozavrik/wsgi.py"
    }
  ]
}
