{
  "version": 2,
  "builds": [
    {
      "src": "chytozavrik/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": ".*",
      "dest": "/chytozavrik/wsgi.py"
    }
  ]
}
